          <section class="our-couses-area pt-90 pb-90">
            <div class="container">
              
              <div class="row">
                        <!-- Start section title -->
                <div class="col-sm-12">
                  <div class="section-title text-center">
                    <h2>Instafamous <span>The Movie</span></h2>
                    <%= image_tag("title-bottom.png", alt: "")%>
                    <p>We need to raise $20,000 in order to bring our idea to life, but we cant do it by ourselves. We need you to reach deep in your pockets.</p>
                  </div>
                </div>
                        <div class="space-50">&nbsp;</div>
                       <!-- End section title -->
                <div class="single-couses">
                            <!-- Start single couses -->
                <% @products.each do |product| %> 
                  <div class="col-sm-4">
                 
                    <div class="single-couses">

                      <div class="single-couses-image-text">
                      
                        <div class="couses-image">
                         <%= link_to image_tag(product.image.url, alt: ""), product_path(product) %>
                        <div class="couses-text text-center">
                          <h3><%= product.title%></h3>
                          <p><%= product.subtitle%></p>
                        </div>
                      </div>
                      <div class="couses-button-and-price">
                        <div class="couses-button">
                          <a><%= link_to 'Donate', product_path(product), class: "btn-md bo-ra-5"%>  
                        </div>

                        <div class="couses-price">
                          <h5><%=  number_to_currency(product.price) %></h5>
                          
                        </div>
                       
                      </div>
                       
                    </div>

                  </div>

                <div class="space-50">&nbsp;</div>       <!-- End single couses -->
                             <!-- Start single couses -->      
                            
                </div>
               <% end %>
              </div>
            
           
            </div>

          </section>

         <!-- end of payment options -->




