<div class="container">
  
  <div class="embed-responsive embed-responsive-16by9">
    <iframe class="embed-responsive-item" src="https://www.youtube.com/embed/cPXx5qkhl0U?autoplay=1"></iframe>
  </div>
</div>



          <section class="our-couses-area pt-90 pb-90">
            <div class="container">
              
              <div class="row">
                        <!-- Start section title -->
                <div class="col-sm-12">
                  <div class="section-title text-center">
                    <h2>Instafamous <span>The Movie</span></h2>
                    <%= image_tag("title-bottom.png", alt: "")%>
                    <p>We need to raise $20,000 in order to bring our idea to life, but we cant do it by ourselves. We need you to reach deep in your pockets.</p>
                  </div>
                </div>
                        <div class="space-50">&nbsp;</div>
                       <!-- End section title -->
                <div class="single-couses">
                            <!-- Start single couses -->
                <% @products.each do |product| %> 
                  <div class="col-sm-4">
                 
                    <div class="single-couses">

                      <div class="single-couses-image-text">
                      
                        <div class="couses-image">
                         <%= link_to image_tag(product.image.url, alt: ""), product_path(product) %>
                        <div class="couses-text text-center">
                          <h3><%= product.title%></h3>
                          <p><%= product.subtitle%></p>
                        </div>
                      </div>
                      <div class="couses-button-and-price">
                        <div class="couses-button">
                          <a><%= link_to 'Donate', product_path(product), class: "btn-md bo-ra-5"%>  
                        </div>

                        <div class="couses-price">
                          <h5><%=  number_to_currency(product.price) %></h5>
                          
                        </div>
                       
                      </div>
                       
                    </div>

                  </div>

                <div class="space-50">&nbsp;</div>       <!-- End single couses -->
                             <!-- Start single couses -->      
                            
                </div>
               <% end %>
              </div>
            
           
            </div>

          </section>

         <!-- end of payment options -->




              <section class="slider-animate-area pb-60">
                <div class="container">
                    <div class="row">
                        <div class="col-sm-12 text-center">
                            <div class="cd-intro">
                                <h1 class="cd-headline clip is-full-width title1 wow zoomIn animated" data-wow-duration="1s" data-wow-delay="300ms" style="visibility: visible; animation-duration: 1s; animation-delay: 300ms; animation-name: zoomIn;">
                                    <span>Discover Whats  </span>
                                    <span class="cd-words-wrapper">
                                        <b class="is-visible">Real</b>
                                        <!-- <b>VS</b> -->
                                        <b> Reality </b>
                                    </span>
                                </h1>
                                <p class="wow zoomIn animated" data-wow-duration="1s" data-wow-delay="600ms" style="visibility: visible; animation-duration: 1s; animation-delay: 600ms; animation-name: zoomIn;">We need to raise $20,000 in order to bring our idea to life, but we cant do it by ourselves. We need you to reach deep in your pockets..</p>
                                <div class="slider-button-area wow zoomIn animated" data-wow-duration="1s" data-wow-delay="900ms" style="visibility: visible; animation-duration: 1s; animation-delay: 900ms; animation-name: zoomIn;">
                                    
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
            </section>

